{
  "name": "gatekeeper",
  "productName": "GateKeeper",
  "version": "0.0.3",
  "description": "Password Generator and a little bit more",
  "main": "src/index.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build": "npm run pack:win64",
    "pack:win64": "electron-packager . GateKeeper --out=dist/win --platform=win32 --arch=x64 app-version=3.0 --icon=./src/views/public/favicon.ico --overwrite"
  },
  "keywords": [],
  "author": {
    "name": "Eiffelware Project",
    "email": "46428773+Peregrinn@users.noreply.github.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "gatekeeper"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "ejs": "^3.1.6",
    "ejs-electron": "^2.1.1",
    "electron-packager": "^15.2.0",
    "electron-router": "^0.5.1",
    "electron-squirrel-startup": "^1.0.0",
    "node-fetch": "^2.6.1",
    "open": "^8.0.8"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "12.0.6"
  }
}
